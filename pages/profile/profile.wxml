<view class="weui-cells">
  <navigator url="./children/personal-information" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="{{userInfo.avatar || '/images/default_avatar.jpg'}}" class="user-info__avatar"></image>
    </view>
    <view class="weui-cell__bd">
      <view class="user-info__name">{{userInfo.nickname}}</view>
      <view class="font-gray-28">查看 / 修改个人信息</view>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
<view class="weui-cells">
  <navigator url="./children/qrcode" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_paper_plane.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd">我的二维码</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
<view class="weui-cells">
  <navigator url="./children/order_progress" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_in_process.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd">借阅进度</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
  <navigator url="./children/order_history" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_time.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd">借阅历史</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
  <navigator url="./children/deposit" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_deposit.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd">我的押金</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
  <navigator url="./children/review" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_review_2.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd">我的书评</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
<view class="weui-cells">
  <navigator url="./children/feedback" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_help.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd">意见反馈</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
  <navigator url="./children/setting" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="../../images/icon_setting.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd">设置</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </navigator>
</view>
<view class="weui-cells">
  <view class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
      <image src="/images/icon_logout.png" class="icon"></image>
    </view>
    <view class="weui-cell__bd" bindtap="onLogout">退出登录</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
</view>